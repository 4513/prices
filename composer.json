{
    "name": "mibo/prices",
    "type": "library",
    "description": "PHP Library for generic and/or custom properties",
    "keywords": [
        "PHP",
        "Properties",
        "Property",
        "ISO",
        "Units",
        "Calculation",
        "Math",
        "Mathematics",
        "Mathematical",
        "Prices",
        "Price",
        "Currencies",
        "Currency",
        "Value"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MiBo\\Prices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Prices\\Tests\\": [
                "tests/Core/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "mibo/properties": "^1.0",
        "mibo/currencies": "^1.0",
        "mibo/vat": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
